{
  "version": 2,
  "builds": [
    {
      "src": "mall-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run dev"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mall-app/dist/$1"
    }
  ]
}
